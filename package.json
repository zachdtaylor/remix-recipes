{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "deploy": "fly deploy --remote-only"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "date-fns": "^2.28.0",
    "dotenv": "^14.2.0",
    "form-data": "^4.0.0",
    "mailgun.js": "^4.1.4",
    "match-sorter": "^6.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.4",
    "concurrently": "^6.5.1",
    "prisma": "^3.8.1",
    "tailwindcss": "^3.0.8",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
